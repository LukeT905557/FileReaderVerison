<AnchorPane prefHeight="480.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UI.AddPhotosController">
   <children>
      <Button fx:id="previousButton" layoutX="438.0" layoutY="440.0" mnemonicParsing="false" onAction="#handlePrevious" text="&lt; Previous" />
      <Button fx:id="nextButton" layoutX="520.0" layoutY="440.0" mnemonicParsing="false" onAction="#handleNext" text="Next &gt;" />
      <ImageView fx:id="artPhoto" fitHeight="396.0" fitWidth="545.0" layoutX="28.0" layoutY="24.0" pickOnBounds="true" preserveRatio="true" />
      <Button fx:id="deleteButton" layoutX="380.0" layoutY="440.0" mnemonicParsing="false" onAction="#handleDelete" text="Delete" />
      <Button fx:id="addPhotoButton" layoutX="171.0" layoutY="440.0" mnemonicParsing="false" onAction="#handleAddPhoto" text="Delete" />
      <TextField fx:id="imagePathField" layoutX="14.0" layoutY="440.0" promptText="Image path..." />
   </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
</AnchorPane>
